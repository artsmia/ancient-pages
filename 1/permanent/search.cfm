<!--- <!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.0 Transitional//EN"><html><head>	<title>Searching...</title>	<cfoutput><meta http-equiv=refresh content="0;url=/uia-bin/uia_query.cgi/postcards/noframes/page_len/10?string=#URLEncodedFormat(query)#&query=#URLEncodedFormat("##BAND( ##FIELD(ONLINE##=yes)  ##FIELD(COLLECTION##=mia) ##OR("&query&"))")#"></cfoutput></head> ---><cfif IsDefined("query") and query is not ''><cfhttp method="post" url="http://www.artsmia.org/uia-bin/uia_query.cgi">  <cfhttpparam type="formfield" name="string" value="#query#">  <cfhttpparam type="formfield" name="query" value="##BAND( ##FIELD(ONLINE##=yes)  ##FIELD(COLLECTION##=mia) ##OR(#query#))">  <cfhttpparam type="formfield" name="noframes" value="x">  <cfhttpparam type="formfield" name="maxdocs" value="200">  <cfhttpparam type="formfield" name="page_len" value="10">  <cfhttpparam type="formfield" name="db_name" value="mia_collection"></cfhttp><cfoutput>  #CFHTTP.FileContent#</cfoutput><cfelse><cfhttp method="post" url="http://www.artsmia.org/uia-bin/uia_query.cgi">  <cfhttpparam type="formfield" name="string" value="Enter search terms here...">  <cfhttpparam type="formfield" name="query" value="##BAND( ##FIELD(ONLINE##=yes)  ##FIELD(COLLECTION##=mia))">  <cfhttpparam type="formfield" name="noframes" value="x">  <cfhttpparam type="formfield" name="maxdocs" value="200">  <cfhttpparam type="formfield" name="page_len" value="10">  <cfhttpparam type="formfield" name="db_name" value="mia_collection"></cfhttp><cfoutput>  #CFHTTP.FileContent#</cfoutput></cfif>